    "# our implementation of U\\b using backsub\n",
    "function ldiv(U::Bidiagonal, b) \n",
    "    T = promote_type(eltype(U), eltype(b))\n",
    "    m,n = size(U) # note m == n\n",
    "    x = zeros(T, n)\n",
    "    if U.uplo == 'U'\n",
